create table NIMASH_TEST_EMPLOYEE (
   id INT NOT NULL ,
   first_name VARCHAR(20) default NULL,
   last_name  VARCHAR(20) default NULL,
   salary     INT  default NULL,
   PRIMARY KEY (id)
);

CREATE SEQUENCE NIMASH_TEST_EMPLOYEE_SEQ
  MINVALUE 1
  MAXVALUE 999999999999999999999999999
  START WITH 1
  INCREMENT BY 1
  CACHE 20;